// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:test@localhost:3306/projects"
}

model Project {
  id          Int    @id @default(autoincrement())
  orgId       Int
  title       String
  description String
}
